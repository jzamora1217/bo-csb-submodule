    "husky": {
        "hooks": {
            "prepare-commit-msg": "./git-commit-message.sh $HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged",
            "commit-msg": "npm run check-commit-msg && npm run lint:quiet && npm run test:ci && npm run svg-sprite"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss}": [
            "prettier --single-quote --write"
        ]
    },
    "scripts": {
        "start": "npm run get-local-env $npm_config_env && vite",
        "serve": "vite preview",
        "cy:open": "npx cypress open",
        "cy:run": "npx cypress run",
        "build": "tsc && vite build",
        "test": "NODE_ENV=test react-scripts test --maxWorkers=2",
        "test:ci": "cross-env CI=true react-scripts test --maxWorkers=2 --colors",
        "eject": "react-scripts eject",
        "check-commit-msg": "node ./check-commit-msg $HUSKY_GIT_PARAMS",
        "lint": "tsc --project ./tsconfig.json && eslint --ext=js,jsx,ts,tsx src",
        "lint:quiet": "tsc --project ./tsconfig.json && eslint --ext=js,jsx,ts,tsx src --quiet",
        "coverage": "npm run test ./src -- --coverage",
        "svg-sprite": "svg-sprite --s --symbol-sprite=icons.svg --symbol-dest=src/components/icon/icons src/components/icon/icons/src/*.svg",
        "cypress:run-ci": "cypress run --env stage=stage1",
        "cypress:run-ci:management": "cypress run --env stage=stage1 --spec 'cypress/e2e/Management'",
        "cypress:run-ci:system": "cypress run --env stage=stage1 --spec 'cypress/e2e/System'",
        "cypress:run-ci:reporting": "cypress run --env stage=stage1 --spec 'cypress/e2e/Reporting'",
        "cypress:run-ci:utilities": "cypress run --env stage=stage1 --spec 'cypress/e2e/Utilities'",
        "cypress:run-ci:jurisdictional": "cypress run --env stage=stage1 --spec 'cypress/e2e/Jurisdictional'",
        "cypress:run": "cypress run --config video=true --env allure=true",
        "cy:parallel": "cypress-parallel -s cypress:run",
        "cypress:stage1": "npx cypress open --env stage=stage1",
        "cypress:stage2": "npx cypress open --env stage=stage2",
        "cypress:stage3": "npx cypress open --env stage=stage3",
        "cypress:stage4": "npx cypress open --env stage=stage4",
        "allure:generate": "allure generate allure-results --clean",
        "allure:open": "allure open allure-report",
        "allure:clear": "rm -r allure-results/ allure-report || true",
        "cy:all": "npx npm-run-all allure:clear cypress:run allure:generate",
        "report:html": "node cucumber-html-report.js",
        "storybook": "start-storybook -s ./public -p 6006",
        "build-storybook": "build-storybook",
        "get-local-env": "node ./get-local-env.js"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "cypress-cucumber-preprocessor": {
        "stepDefinitions": "cypress/e2e",
        "cucumberJson": {
            "generate": true,
            "outputFolder": "cypress/cucumber-json",
            "filePrefix": "",
            "fileSuffix": ".cucumber"
        }
    }
}
